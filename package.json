{
	"name": "vetnotes-web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy:mvp": "npm run build && firebase deploy -P mvp --only hosting",
		"deploy:pro": "npm run build && firebase deploy -P pro --only hosting"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"autoprefixer": "^10.4.23",
		"daisyui": "^4.12.24",
		"jsdom": "^27.4.0",
		"postcss": "^8.5.6",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.19",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^4.0.18",
		"wrangler": "^4.58.0"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"@googlemaps/js-api-loader": "^2.0.2",
		"@tsparticles/confetti": "^3.9.1",
		"@xenova/transformers": "^2.17.2",
		"dayjs": "^1.11.19",
		"js-sha3": "^0.9.3"
	}
}
